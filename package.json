{
  "name": "tdd-commentbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:ci": "cross-env CI=true yarn test --coverage",
    "test:story": "jest test /tests --testMatch '**/*.spec.js' --config tests/jest.config.js",
    "test:story:update": "yarn test:story -u",
    "lint:fix": "eslint src --fix",
    "cy:open": "cypress open",
    "cy:e2e": "cypress run",
    "cy:e2e:update": "yarn cy:e2e --env updateSnapshots=true",
    "mock": "json-server --watch mock/db.json --routes mock/routes.json --port 8080",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "proxy": "http://localhost:8080",
  "dependencies": {
    "@storybook/addon-actions": "^5.3.7",
    "@storybook/addon-links": "^5.3.7",
    "@storybook/addon-storyshots": "^5.3.8",
    "@storybook/addon-storyshots-puppeteer": "^5.3.8",
    "@storybook/addons": "^5.3.7",
    "@storybook/react": "^5.3.7",
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd-mobile": "^2.3.1",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.12.2",
    "cross-env": "^7.0.0",
    "customize-cra": "^0.9.1",
    "cypress": "^3.8.2",
    "cypress-image-snapshot": "^3.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^4.2.1",
    "json-server": "^0.15.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.2",
    "prettier": "^1.19.1",
    "puppeteer": "^2.1.0",
    "react-app-rewired": "^2.1.5",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.3",
    "redux-saga-test-plan": "^4.0.0-rc.2",
    "storybook-react-router": "^1.0.8"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "jsx-a11y/accessible-emoji": "off"
    },
    "globals": {
      "Cypress": true,
      "cy": true
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": false
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "post-commit": "git update-index --again",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test:ci"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
